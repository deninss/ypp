<Window x:Class="WpfApp1.Pages.QRCode"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1.Pages"
      mc:Ignorable="d" 
      MinHeight="300" MinWidth="300"  MaxHeight="300" MaxWidth="300"
      Title="QRCode">

    <Grid Background="#f6f6f6">
        <Label Content="Назад" Margin="20,20,0,0" FontSize="14" Opacity="0.6" FontWeight="Bold" MouseDown="TransitionBack" HorizontalAlignment="Left" Width="72" Height="31" VerticalAlignment="Top"/>
        <TextBlock x:Name="AAA" HorizontalAlignment="Center" Margin="0,20,0,0" FontSize="20" Foreground="Black" FontWeight="Bold" Text="QR code" VerticalAlignment="Top"/>
        <Image x:Name="ImageQRCode" Margin="20,56,31,15"/>
    </Grid>
</Window>
